js",[],"/Users/carolinec/Desktop/zeroblocks/src/actions/memberActions.js",[],"/Users/carolinec/Desktop/zeroblocks/src/reducers/index.js",[],"/Users/carolinec/Desktop/zeroblocks/src/reducers/memberReducer.js",["97"],"/Users/carolinec/Desktop/zeroblocks/src/index.js",["98"],"import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\n\nimport 'materialize-css/dist/css/materialize.min.css';\nimport M from 'materialize-css/dist/js/materialize.min.js';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n",["99","100"],"/Users/carolinec/Desktop/zeroblocks/src/components/layout/SearchBar.js",[],"/Users/carolinec/Desktop/zeroblocks/src/components/updates/EditUpdateModal.js",["101"],"import React, { useState, useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport PropTypes from 'prop-types';\nimport MemberSelect from '../members/MemberSelect';\nimport { editUpdates, setCurrent } from '../../actions/updateActions';\nimport M from 'materialize-css/dist/js/materialize.min.js/';\n\nconst EditUpdateModal = ({ current, editUpdates }) => {\n    const [message, setMessage] = useState('');\n    const [attention, setAttention] = useState(false);\n    const [member, setMember] = useState('');\n\n    useEffect(() => {\n        if(current) {\n            setMessage(current.message);\n            setAttention(current.attention);\n            setMember(current.member);\n        }\n    }, [current]);\n\n    const onSubmit = () => {\n        if(message === '' || member === '') {\n            M.toast({ html: 'Please enter your update and name'})\n        } else {\n            const editUpd = {\n                id: current.id,\n                message,\n                attention,\n                member,\n                date: new Date()\n            };\n\n            editUpdates(editUpd);\n            M.toast({ html: `This update was edited by ${member}`})\n            // Clear Fields\n            setMessage('');\n            setMember('');\n            setAttention(false);\n        }\n    };\n    return (\n        <div id=\"edit-update-modal\" className=\"modal\">\n            <div className=\"modal-content\">\n                <h5>What did you work on since yesterday's standup?</h5>\n                <div className=\"row\">\n                    <div className=\"input-field\">\n                        <input\n                        type=\"text\"\n                        name=\"message\"\n                        value={message}\n                        onChange={e => setMessage(e.target.value)}\n                        />\n                    </div>\n                </div>\n\n                <div className=\"row\">\n                    <div className=\"input-field\">\n                        <select\n                        name=\"member\"\n                        value={member}\n                        className=\"browser-default\"\n                        onChange={e => setMember(e.target.value)}>\n                        <option value=\"\" disabled>\n                            Add your name\n                        </option>\n                        <MemberSelect />\n                        </select>\n                    </div>\n                </div>\n\n                <div className=\"row\">\n                    <div className=\"input-field\">\n                        <p>\n                            <label>\n                                <input\n                                type=\"checkbox\"\n                                className=\"filled-in\"\n                                checked={attention}\n                                value={attention}\n                                onChange={e => setAttention(!attention)} />\n                                <span>Incomplete - To discuss further</span>\n                            </label>\n                        </p>\n                    </div>\n                </div>\n            </div>\n            <div className=\"modal-footer\">\n                <button type=\"button\" onClick={onSubmit} className=\"modal-close waves-effect blue btn\">Enter</button>\n            </div>\n        </div>\n    )\n};\n\nEditUpdateModal.propTypes = {\n    current: PropTypes.object,\n    editUpdates: PropTypes.func.isRequired\n}\n\nconst mapStateToProps = state => ({\n    current: state.update.current\n});\n\nexport default connect(mapStateToProps, { editUpdates })(EditUpdateModal);\n","/Users/carolinec/Desktop/zeroblocks/src/components/updates/AddUpdateModal.js",[],"/Users/carolinec/Desktop/zeroblocks/src/store.js",[],["102","103"],"/Users/carolinec/Desktop/zeroblocks/src/actions/updateActions.js",[],"/Users/carolinec/Desktop/zeroblocks/src/components/layout/AddBtn.js",[],"/Users/carolinec/Desktop/zeroblocks/src/components/updates/UpdateItem.js",[],"/Users/carolinec/Desktop/zeroblocks/src/reducers/updateReducer.js",["104"],"import {\n    GET_UPDATES,\n    SET_LOADING,\n    UPDATES_ERROR,\n    ADD_UPDATE,\n    EDIT_UPDATE,\n    SEARCH_UPDATES,\n    DELETE_UPDATE,\n    SET_CURRENT,\n    CLEAR_CURRENT\n} from '../actions/types';\n\nconst initialState = {\n    updates: null,\n    current: null,\n    loading: false,\n    error: null\n};\n\nexport default (state = initialState, action) => {\n    switch(action.type) {\n        case GET_UPDATES:\n            return {\n                ...state,\n                updates: action.payload,\n                loading: false\n            };\n        case ADD_UPDATE:\n            return {\n                ...state,\n                updates: [...state.updates, action.payload],\n                loading: false\n            };\n        case EDIT_UPDATE:\n            return {\n                ...state,\n                updates: state.updates.map(update => update.id === action.payload.id ? action.payload : update )\n            };\n        case SEARCH_UPDATES:\n            return {\n                ...state,\n                updates: action.payload\n            };\n        case DELETE_UPDATE:\n            return {\n                ...state,\n                updates: state.updates.filter(update => update.id !== action.payload),\n                loading: false\n            };\n        case SET_CURRENT:\n            return {\n                ...state,\n                current: action.payload\n            };\n        case CLEAR_CURRENT:\n            return {\n                ...state,\n                current: null\n            };\n        case SET_LOADING:\n            return {\n                ...state,\n                loading: true\n            };\n        case UPDATES_ERROR:\n            console.error(action.payload)\n            return {\n                ...state,\n                error: action.payload\n            };\n        default:\n        return state;\n    }\n};","/Users/carolinec/Desktop/zeroblocks/src/components/members/MemberItem.js",["105"],"import React from 'react';\nimport { connect } from 'react-redux';\nimport PropTypes from 'prop-types';\nimport { deleteMember } from '../../actions/memberActions';\nimport M from 'materialize-css/dist/js/materialize.min.js/';\n\nconst MemberItemModal = ({ member: { firstName, lastName, id }, deleteMember }) => {\n    const onDelete = () => {\n        deleteMember(id);\n        M.toast({ html: 'Team member deleted'})\n    };\n\n    return (\n        <div>\n            <li className=\"collection-item\">\n                <div>\n                    {firstName} {lastName}\n                    <a href=\"\" aria-label=\"delete\" className=\"secondary-content\" onClick={onDelete}>\n                        <i className=\"material-icons grey-text\">delete</i>\n                    </a>\n                </div>\n            </li>\n        </div>\n    )\n};\n\nMemberItemModal.propTypes = {\n    member: PropTypes.object.isRequired,\n    deleteMember: PropTypes.func.isRequired\n};\n\nexport default connect(null, { deleteMember })(MemberItemModal);\n","/User